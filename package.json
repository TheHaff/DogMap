{
  "name": "DogMap",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://thehaff.github.io/DogMap",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "ignore": [
      "dist/",
      "target/",
      "build/",
      "out/",
      "node_modules/",
      "*.min.js",
      "*.min.css",
      "*.bundle.js",
      "pnpm-lock.yaml",
      "package-lock.json",
      "yarn.lock",
      "search-worker/target/",
      "search-worker/pkg/",
      "src/wasm/",
      "*.log",
      "coverage/",
      ".cache",
      ".parcel-cache",
      "tmp/",
      "temp/",
      ".DS_Store"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build:wasm": "cd search-worker && wasm-pack build --target web && cpx \"pkg/*\" ../src/wasm/",
    "build": "pnpm run build:wasm && tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "predeploy": "vite build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "ts-patch install"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.5.1",
    "framer-motion": "^12.19.1",
    "jotai": "^2.12.5",
    "jotai-effect": "^2.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "tailwindcss": "^4.1.11",
    "typia": "^9.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@faker-js/faker": "^9.8.0",
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/web-worker": "^3.2.4",
    "cpx": "^1.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.4.2",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
